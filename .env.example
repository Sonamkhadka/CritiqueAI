
# AI Service API Keys

# Required for basic functionality with free models
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Optional - Only needed for direct access to premium models
OPENAI_API_KEY=your_openai_api_key_here
DEEPSEEK_API_KEY=your_deepseek_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here

# Session Configuration (Optional)
SESSION_SECRET=your_session_secret_here

# Default Model Configuration
# Available models: openrouter, openai, deepseek, gemini
DEFAULT_AI_MODEL=openrouter
DEFAULT_OPENROUTER_MODEL=mistralai/mistral-7b-instruct:free

# Available OpenRouter Models (comma-separated list)
# You can customize this list to add/remove models as needed
OPENROUTER_MODELS=mistralai/mistral-7b-instruct:free,meta-llama/llama-3.3-70b-instruct:free,google/gemini-2.5-pro-exp-03-25:free,deepseek/deepseek-r1-zero:free,openai/gpt-4o-mini

